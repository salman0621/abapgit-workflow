      <item>            VBSK_E           = VBSK</item>
      <item>       TABLES</item>
      <item>            XKOMFK           = XKOMFK</item>
      <item>            XTHEAD           = XTHEAD</item>
      <item>            XVBFS            = XVBFS</item>
      <item>            XVBSS            = XVBSS</item>
      <item>            XVBRK            = XVBRK</item>
