     <SOURCE>
      <item/>
      <item>  CALL FUNCTION &apos;RV_INVOICE_DOCUMENT_ADD&apos;</item>
      <item>       EXPORTING</item>
      <item>            VBSK_I           = VBSK</item>
      <item>            WITH_POSTING     = &apos;A&apos;</item>
      <item>            PREISFINDUNGSART = CALC_TYPE</item>
